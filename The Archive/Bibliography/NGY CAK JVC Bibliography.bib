@article{kokenyPPARgTGFvMajorRegulators2021,
  title = {{{PPAR$\gamma$}} and {{TGF$\beta$-Major Regulators}} of {{Metabolism}}, {{Inflammation}}, and {{Fibrosis}} in the {{Lungs}} and {{Kidneys}}},
  author = {K{\"o}k{\'e}ny, G{\'a}bor and Calvier, Laurent and Hansmann, Georg},
