  author = {{Gomes-Neto}, Mansueto and Lopes, Jordana Moura and Concei{\c c}{\~a}o, Cristiano Sena and Araujo, Anderson and Brasileiro, Al{\'e}cio and Sousa, Camila and Carvalho, Vitor Oliveira and Arcanjo, Fabio Luciano},
