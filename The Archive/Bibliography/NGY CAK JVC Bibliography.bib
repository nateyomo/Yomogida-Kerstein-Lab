  author = {Astrup, Arne and Magkos, Faidon and Bier, Dennis M. and Brenna, J. Thomas and {de Oliveira Otto}, Marcia C. and Hill, James O. and King, Janet C. and Mente, Andrew and Ordovas, Jose M. and Volek, Jeff S. and Yusuf, Salim and Krauss, Ronald M.},
