@article{mitchellPhysiologicalEffectsPhysical2017,
  title = {Physiological Effects of Physical Therapy Interventions on Lumbar Intervertebral Discs: {{A}} Systematic Review},
  shorttitle = {Physiological Effects of Physical Therapy Interventions on Lumbar Intervertebral Discs},
  author = {Mitchell, Ulrike H. and Helgeson, Kevin and Mintken, Paul},
