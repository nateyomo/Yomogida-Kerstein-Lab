@article{saturnoValidityReliabilityGuidelines2003,
  title = {Validity and Reliability of Guidelines for Neck Pain Treatment in Primary Health Care. {{A}} Nationwide Empirical Analysis in {{Spain}}},
  author = {Saturno, Pedro J. and Medina, Francesc and Valera, Fermin and Montilla, Joaquina and Escolar, Pilar and Gasc{\'o}n, Juan J.},
