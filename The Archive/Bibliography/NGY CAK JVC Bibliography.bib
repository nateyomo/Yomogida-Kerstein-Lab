  pmid = {26360531},
  keywords = {Adult,Arthrometry Articular,Biomechanical Phenomena,Equipment Design,Female,Humans,Inclinometer,Male,Patient Positioning,Predictive Value of Tests,prone,Prone Position,range of motion,Range of Motion Articular,reliability,Reproducibility of Results,shoulder,Shoulder Joint,Supine Position,Young Adult},
