  keywords = {Adaptation Physiological,Biomechanical Phenomena,Humans,Models Theoretical,Physical Therapy Modalities,Stress Mechanical,To Read},
  file = {/Users/nathanielyomogida/Zotero/storage/FU9SY848/Mueller_Maluf (2002) Tissue adaptation to physical stress.pdf}
}

