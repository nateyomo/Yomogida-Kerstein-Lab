  pmcid = {PMC3109687},
  pmid = {21886423},
  keywords = {Conservative management,Review,Surgical management,Thoracic outlet syndrome,To Read},
  file = {C\:\\Users\\black\\Zotero\\storage\\EAVQ82E4\\Hooper et al (2010) Thoracic outlet syndrome.pdf;C\:\\Users\\black\\Zotero\\storage\\EIK2WZ7X\\hooper2010.pdf.pdf}
