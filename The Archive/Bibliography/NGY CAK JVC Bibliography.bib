@article{fariMusculoskeletalPainRelated2022,
  title = {Musculoskeletal Pain Related to Surfing Practice: {{Which}} Role for Sports Rehabilitation Strategies? {{A}} Cross-Sectional Study},
  shorttitle = {Musculoskeletal Pain Related to Surfing Practice},
  author = {Far{\`i}, Giacomo and Santagati, Dario and Macchiarola, Dario and Ricci, Vincenzo and Di Paolo, Stefano and Caforio, Laura and Invernizzi, Marco and Notarnicola, Angela and Megna, Marisa and Ranieri, Maurizio},
  year = {2022},
  journal = {Journal of Back and Musculoskeletal Rehabilitation},
  volume = {35},
