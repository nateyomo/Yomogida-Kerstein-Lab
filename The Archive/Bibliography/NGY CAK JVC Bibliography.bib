  keywords = {Acetaminophen,Adult,Aged,Analgesics,{Anti-Inflammatory Agents, Non-Steroidal},Dietary Supplements,Double-Blind Method,Female,Fish Oils,Humans,Male,Middle Aged,Osteoarthritis,Pain,Phytotherapy,Placebos,Urtica dioica,Vitamin E},
