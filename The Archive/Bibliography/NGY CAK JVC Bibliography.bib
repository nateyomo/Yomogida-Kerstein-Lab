@article{williamsonPosttraumaticStressDisorder2021,
  title = {Posttraumatic {{Stress Disorder}} and {{Anxiety-Related Conditions}}},
  author = {Williamson, John B. and Jaffee, Michael S. and Jorge, Ricardo E.},
