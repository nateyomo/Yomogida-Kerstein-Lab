@article{kongEffectsNeuromuscularElectrical2022,
  title = {Effects of {{Neuromuscular Electrical Stimulation}} and {{Blood Flow Restriction}} in {{Rehabilitation}} after {{Anterior Cruciate Ligament Reconstruction}}},
  author = {Kong, Doo-Hwan and Jung, Won-Sang and Yang, Sang-Jin and Kim, Jin-Goo and Park, Hun-Young and Kim, Jisu},
