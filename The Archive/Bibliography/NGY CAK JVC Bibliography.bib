  pmid = {26174323},
  keywords = {{Adaptation, Physiological},Adult,Cold Temperature,Exercise,Humans,Hypertrophy,Male,Metabolism,Muscle Strength,{Muscle, Skeletal},Recovery of Function,Resistance Training,Signal Transduction,Water,Young Adult},
  file = {C:\Users\black\Zotero\storage\WS6RAV8E\Roberts et al (2015) Post-exercise cold water immersion attenuates acute anabolic signalling and.pdf}
