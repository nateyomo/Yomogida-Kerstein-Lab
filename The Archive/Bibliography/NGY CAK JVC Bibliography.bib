  keywords = {Diagnosis,Elbow,Elbow Tendinopathy,Finish reading,Humans,Magnetic Resonance Imaging,Musculoskeletal Diseases,Sensitivity,Sensitivity and Specificity,Specificity,Tendinopathy,Tennis elbow,Ultrasonography},
  file = {C:\Users\black\Zotero\storage\BP5SBJBQ\Karanasios et al (2022) Diagnostic accuracy of examination tests for lateral elbow tendinopathy (LET) -.pdf}
}

