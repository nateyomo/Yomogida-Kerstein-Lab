@article{mchardyGolfInjuriesReview2006,
  title = {Golf Injuries: A Review of the Literature},
  shorttitle = {Golf Injuries},
  author = {McHardy, Andrew and Pollard, Henry and Luo, Kehui},
  year = {2006},
  journal = {Sports Medicine (Auckland, N.Z.)},
