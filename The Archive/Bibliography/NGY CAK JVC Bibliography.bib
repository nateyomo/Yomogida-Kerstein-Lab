@article{al-wailiNaturalHoneyLowers2004,
  title = {Natural Honey Lowers Plasma Glucose, {{C-reactive}} Protein, Homocysteine, and Blood Lipids in Healthy, Diabetic, and Hyperlipidemic Subjects: Comparison with Dextrose and Sucrose},
  shorttitle = {Natural Honey Lowers Plasma Glucose, {{C-reactive}} Protein, Homocysteine, and Blood Lipids in Healthy, Diabetic, and Hyperlipidemic Subjects},
