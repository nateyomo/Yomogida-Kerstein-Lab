@article{luDietaryFatIntake2017,
  title = {Dietary {{Fat Intake}} and {{Radiographic Progression}} of {{Knee Osteoarthritis}}: {{Data From}} the {{Osteoarthritis Initiative}}},
  shorttitle = {Dietary {{Fat Intake}} and {{Radiographic Progression}} of {{Knee Osteoarthritis}}},
