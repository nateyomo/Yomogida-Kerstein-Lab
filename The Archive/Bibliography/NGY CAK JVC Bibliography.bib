@article{mastromarchiFirstRibDysfunction2021,
  title = {First Rib Dysfunction in Patients with Neck and Shoulder Pain: A {{Delphi}} Investigation},
  shorttitle = {First Rib Dysfunction in Patients with Neck and Shoulder Pain},
