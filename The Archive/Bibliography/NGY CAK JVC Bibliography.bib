@article{czupponVariablesAssociatedReturn2014,
  title = {Variables Associated with Return to Sport Following Anterior Cruciate Ligament Reconstruction: A Systematic Review},
  shorttitle = {Variables Associated with Return to Sport Following Anterior Cruciate Ligament Reconstruction},
  author = {Czuppon, Sylvia and Racette, Brad A. and Klein, Sandra E. and {Harris-Hayes}, Marcie},
