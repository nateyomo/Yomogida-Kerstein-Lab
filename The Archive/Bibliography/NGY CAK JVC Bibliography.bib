  author = {Choi, Hoon Sung and Kwon, Hyuki and Cho, Keun-Hyok and Thuc, Le Cong and Park, So Young and Kim, Nam-Hun and Kim, Wae-Jung and Kim, Yun-Ki and Jeon, Woo-Seok and Lee, Ju-Ah and Kim, Hyoung-Chun and Cho, Eun-Hee},
  year = {2020},
