@article{xiaoEffectsColdWater2023,
  title = {Effects of Cold Water Immersion after Exercise on Fatigue Recovery and Exercise Performance--Meta Analysis},
  author = {Xiao, Feiyan and Kabachkova, Anastasiia V. and Jiao, Lu and Zhao, Huan and Kapilevich, Leonid V.},
  year = {2023},
