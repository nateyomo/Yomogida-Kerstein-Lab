@article{wardEpidemiologyPathophysiologyMultiple2022,
  title = {Epidemiology and {{Pathophysiology}} of {{Multiple Sclerosis}}},
  author = {Ward, Melanie and Goldman, Myla D.},
  year = {2022},
  month = aug,
  journal = {CONTINUUM: Lifelong Learning in Neurology},
  volume = {28},
  number = {4},
