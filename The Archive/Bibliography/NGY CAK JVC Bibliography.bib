@article{sejvarZikaVirusOther2018,
  title = {Zika {{Virus}} and {{Other Emerging Arboviral Central Nervous System Infections}}},
  author = {Sejvar, James J.},
  year = {2018},
  month = oct,
  journal = {CONTINUUM: Lifelong Learning in Neurology},
