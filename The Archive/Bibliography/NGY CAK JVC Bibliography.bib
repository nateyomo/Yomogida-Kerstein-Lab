}

@article{panickerLowerUrinaryTract2020,
  title = {Lower {{Urinary Tract}} and {{Bowel Dysfunction}} in {{Neurologic Disease}}},
  author = {Panicker, Jalesh N. and Sakakibara, Ryuji},
  year = {2020},
  month = feb,
  journal = {CONTINUUM: Lifelong Learning in Neurology},
  volume = {26},
  number = {1},
