@article{grandpierreBarriersFacilitatorsCultural2018,
  title = {Barriers and Facilitators to Cultural Competence in Rehabilitation Services: A Scoping Review},
  shorttitle = {Barriers and Facilitators to Cultural Competence in Rehabilitation Services},
  author = {Grandpierre, Viviane and Milloy, Victoria and Sikora, Lindsey and Fitzpatrick, Elizabeth and Thomas, Roanne and Potter, Beth},
