@article{nandaSIP68AbbreviatedSickness2003,
  title = {The {{SIP68}}: An Abbreviated Sickness Impact Profile for Disability Outcomes Research},
  shorttitle = {The {{SIP68}}},
  author = {Nanda, Upasana and McLendon, Patricia M. and Andresen, Elena M. and Armbrecht, Eric},
