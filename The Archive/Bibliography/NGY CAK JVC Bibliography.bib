@article{doughtyToxicMyopathies2019,
  title = {Toxic {{Myopathies}}},
  author = {Doughty, Christopher T. and Amato, Anthony A.},
  year = {2019},
  month = dec,
  journal = {CONTINUUM: Lifelong Learning in Neurology},
  volume = {25},
